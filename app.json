{
  "expo": {
    "name": "Elite Prep",
    "slug": "elite-prep-mobile-client-new",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elite-prep.app",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Elite Prep to access your photos.",
        "NSCameraUsageDescription": "Allow Elite Prep to access your camera.",
        "NSMicrophoneUsageDescription": "Allow Elite Prep to access your microphone.",
        "UIBackgroundModes": ["remote-notification"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["com.elite-prep.app"]
          }
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.elite-prep.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6cea3a74-0df9-4058-9f11-5ff0b03a5387"
      }
    },
    "owner": "jmaydwell1"
  }
}
